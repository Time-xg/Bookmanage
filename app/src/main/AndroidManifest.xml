<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="top.timewl.bookmanage">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.BookManage">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ScanActivity">

        </activity>
        <activity android:name=".activity.SearchActivity">

        </activity>
        <activity android:name=".activity.BookActivity"></activity>
        <activity android:name=".activity.BorrowActivity"></activity>
        <activity android:name=".activity.RegisteredActivity"></activity>
        <activity android:name=".activity.LoginActivity">

        </activity>
        <activity android:name=".activity.AddBookActivity">

        </activity>
    </application>

</manifest>